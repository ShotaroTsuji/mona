;; prompt string
(define (mona-prompt-string)
  (string-append "mona:"
                 (fs-path (fs-current-directory))
                 ">"))

(define (recv name)
  (call-process (string-append "/APPS/FRECV.EX5 " name)))

(define (recvrun name)
  (call-process (string-append "/APPS/FRECV.EX5 " name))
  (call-process (string-append "/MEM/" name)))

(define (mosh)
  (call-process "/APPS/FRECV.EX5 MOSH.EXE")
